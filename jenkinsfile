pipeline{
  agent any


  stages{
    stage('Checkout'){
      steps{
        git branch: 'main', url: 'https://github.com/Gowri0126/Test-devops' 
      }
    }

    stage('Compile'){
      steps {
        echo 'Compiling the Java program...'
        sh 'javac Test.java'
      }
    }

    stage('Run'){
      steps{
        echo 'Running the Java program...'
        sh 'java Test'
      }
    }
  }

  post{
    success{
      echo 'Build and execution successful..'
    }
    failure{
      echo 'Build failed..'
    }
  }
}
